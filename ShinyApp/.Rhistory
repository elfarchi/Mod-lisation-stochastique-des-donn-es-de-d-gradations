T1 = 1000
k = (1:1000)
T = 10
t = (k/T1)*T
M = matrix(0,T1,T1)
for(i in (1:T1)){
M[i,]= (sqrt(8*T)/((2*k+1)*pi))*sin(((2*k+1)*pi*t[i])/(2*T))
}
Z_k = rnorm(T1)
B_t = matrix(0,1,T1)
for (k in (1:T1)){
